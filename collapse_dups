#!/bin/env perl6

my %count-for;

for lines() -> $line
{
    my ($count, $seq) = parse-line($line); 
    %count-for{$seq} += $count;
}

%count-for.sort( .value ) -> $pair
{
    "$pair.value()\t$pair.key".say; 
}

sub parse-line ($line)
{
    if $line ~~ / (\d+) \s+ (\w+) /
    {
        return ($0, $1);
    }
    else {
        die 'could not parse line';
    }
}
